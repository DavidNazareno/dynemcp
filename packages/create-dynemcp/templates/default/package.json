{
  "name": "dynemcp-project",
  "version": "0.1.0",
  "description": "A Model Context Protocol (MCP) server",
  "type": "module",
  "main": "dist/server.js",
  "imports": {
    "#app/*": "./src/*"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development npx @modelcontextprotocol/inspector serve src/index.ts",
    "build": "dynebuild",
    "start": "node dist/server.js",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@repo/dynebuild": "workspace:*",
    "@repo/dynemcp": "workspace:*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.20.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "packageManager": "pnpm@10.9.0"
}
