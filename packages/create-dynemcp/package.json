    "@types/node": "^20.19.0",
    "@types/validate-npm-package-name": "^4.0.2",
    "eslint": "^9.28.0",
    "execa": "^9.6.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
