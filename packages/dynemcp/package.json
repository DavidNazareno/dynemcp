{
  "name": "@dynemcp/dynemcp",
  "version": "0.0.20-canary.20250626T18090",
  "description": "Complete MCP framework with server runtime and build system",
  "private": false,
  "type": "module",
  "main": "./dist/lib/index.js",
  "module": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "bin": {
    "dynemcp": "./dist/cli.cjs"
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c && tsc --emitDeclarationOnly --outDir dist",
    "clean": "rm -rf dist"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/lib/index.ts",
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js",
      "default": "./dist/lib/index.js"
    },
    "./server": {
      "development": "./src/lib/server/index.ts",
      "types": "./dist/lib/server/index.d.ts",
      "import": "./dist/lib/server/index.js",
      "default": "./dist/lib/server/index.js"
    },
    "./build": {
      "development": "./src/lib/build/index.ts",
      "types": "./dist/lib/build/index.d.ts",
      "import": "./dist/lib/build/index.js",
      "default": "./dist/lib/build/index.js"
    }
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.2",
    "chalk": "4.1.2",
    "cors": "^2.8.5",
    "esbuild": "0.25.5",
    "express": "4.21.2",
    "jsonwebtoken": "^9.0.2",
    "tslib": "^2.3.0",
    "yargs": "17.7.2",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.14.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.30",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.16.2"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "server",
    "build",
    "bundler",
    "esbuild",
    "typescript",
    "framework"
  ]
}
