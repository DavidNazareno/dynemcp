{
  "name": "@dynemcp/dynemcp",
  "version": "0.0.20-canary.20250626T18090",
  "description": "Complete MCP framework with server runtime and build system",
  "private": false,
  "type": "module",
  "main": "./dist/lib/index.js",
  "module": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "bin": {
    "dynemcp": "./dist/cli.cjs"
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c && tsc --emitDeclarationOnly --outDir dist",
    "clean": "rm -rf dist"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/lib/index.ts",
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js",
      "default": "./dist/lib/index.js"
    },
    "./server": {
      "development": "./src/lib/server/index.ts",
      "types": "./dist/lib/server/index.d.ts",
      "import": "./dist/lib/server/index.js",
      "default": "./dist/lib/server/index.js"
    },
    "./build": {
      "development": "./src/lib/build/index.ts",
      "types": "./dist/lib/build/index.d.ts",
      "import": "./dist/lib/build/index.js",
      "default": "./dist/lib/build/index.js"
    }
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.3",
    "chalk": "5.4.1",
    "cors": "^2.8.5",
    "esbuild": "0.25.5",
    "express": "5.1.0",
    "express-oauth2-jwt-bearer": "^1.6.1",
    "express-rate-limit": "7.5.1",
    "jsonwebtoken": "^9.0.2",
    "tslib": "^2.8.1",
    "yargs": "18.0.0",
    "zod": "^3.25.71",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.15.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.10",
    "tsx": "^4.20.3"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "server",
    "build",
    "bundler",
    "esbuild",
    "typescript",
    "framework"
  ]
}
